{
	"tags": {
		"allowUnknownTags": true
	},
	"source": {
		"include": ["generated"],
		"exclude": ["generated/node_modules"],
		"includePattern": ".+\\.js(doc)?$",
		"excludePattern": "(^|\\/|\\\\)_"
	},
	"plugins": ["plugins/markdown"],
	"templates": {
		"cleverLinks": true,
		"monospaceLinks": false,
		"systemName"      : "Roboliq Protocol",
		"footer"          : "",
		"copyright"       : "Roboliq Copyright Â© 2016 Ellis Whitehead",
		"navType"         : "inline",
		"theme"           : "cerulean",
		"linenums"        : true,
		"collapseSymbols" : false,
		"inverseNav"      : true,
		"outputSourceFiles": false,
		"search": true
	},
	"opts": {
		"destination": "./out/protocol"
	}
}
