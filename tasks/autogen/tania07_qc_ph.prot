description: |
  Titrate a range of volumes of GFP using tips 1 to 4 into a 384 well flat-bottom plate, total volume of 75ul

labware:
  trough1:
    model: troughModel_100ml
    location: R5
  trough2:
    model: troughModel_100ml
    location: R6
  mixPlate:
    model: plateModel_384_square
    location: P3

wellGroup:
- name: destGroup1
  well: mixPlate()
- name: destGroup2
  well: mixPlate()
- name: destGroup3
  well: mixPlate()
- name: destGroup4
  well: mixPlate()

substance:
- name: water
- name: dyeLight
  description: OrangeG 0.08g/L

source:
- name: water
  well: trough1(C01|F01)
- name: dyeLight
  well: trough2(C01|F01)

protocol:
# first water, then dye with flush between, wet dispense
- titrate:
    allOf:
    - source: water
      cleanBetween: none
      pipettePolicy: Roboliq_Water_Dry_1000
    - source: dyeLight
      amount: [5ul, 10ul, 15ul, 20ul, 25ul, 30ul, 35ul, 40ul]
      cleanBetween: flush
      pipettePolicy: Roboliq_Water_Wet_1000
    destination: destGroup1
    amount: 75ul
    replicates: 3
    tip: [1, 2, 3, 4]
# first water, then dye without flush between, wet dispense
- titrate:
    allOf:
    - source: water
      cleanBetween: none
      pipettePolicy: Roboliq_Water_Dry_1000
    - source: dyeLight
      amount: [5ul, 10ul, 15ul, 20ul, 25ul, 30ul, 35ul, 40ul]
      cleanBetween: none
      pipettePolicy: Roboliq_Water_Wet_1000
    destination: destGroup2
    amount: 75ul
    replicates: 3
    tip: [1, 2, 3, 4]
# first water, then dye with flush between, air dispense
- titrate:
    allOf:
    - source: water
      cleanBetween: none
      pipettePolicy: Roboliq_Water_Dry_1000
    - source: dyeLight
      amount: [5ul, 10ul, 15ul, 20ul, 25ul, 30ul, 35ul, 40ul]
      cleanBetween: flush
      pipettePolicy: Roboliq_Water_Air_1000
    destination: destGroup3
    amount: 75ul
    replicates: 2
    tip: [1, 2, 3, 4]
# first water, then dye without flush between, air dispense
- titrate:
    allOf:
    - source: water
      cleanBetween: none
      pipettePolicy: Roboliq_Water_Dry_1000
    - source: dyeLight
      amount: [5ul, 10ul, 15ul, 20ul, 25ul, 30ul, 35ul, 40ul]
      cleanBetween: none
      pipettePolicy: Roboliq_Water_Air_1000
    destination: destGroup4
    amount: 75ul
    replicates: 2
    tip: [1, 2, 3, 4]
