# - put balance plate into centrifuge bay 4
# - put mix plate into centrifuge bay 2
# - run the centrifuge
labware:
  plate1:
    model: plateModel_384_square
    location: P3
  plate2:
    model: plateModel_384_square
    location: P2

protocol:
- transportLabware:
    object: plate2
    site: CENTRIFUGE_4
- transportLabware:
    object: plate1
    site: CENTRIFUGE_2
- carousel.close:
    device: mario__Centrifuge
- evoware.centrifuge.run:
    program:
      rpm: 3000
      duration: 300
      temperature: 25
