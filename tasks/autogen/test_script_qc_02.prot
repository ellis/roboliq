description: |
  Test small volumes in small tips.
  Distribute 30ul of diluted dye, measure absorbance, then titrate various small volumes of dye using the small tips.

labware:
  trough1:
    model: troughModel_100ml
    location: R5
  trough2:
    model: troughModel_100ml
    location: R6
  plate1:
    model: plateModel_384_round
    location: P3

substance:
- name: dye
- name: water

source:
- name: dyeLight
  well: trough1(A01|H01)
  substance:
  - name: dye
    amount: 1/10
  - name: water
- name: dyeDark
  well: trough2(A01|H01)
  substance:
  - name: dye

protocol:
- distribute:
    source: dyeLight
    destination: plate1(A01|H12)
    amount: 30ul
    pipettePolicy: Roboliq_Water_Air_1000
- measureAbsorbance:
    object: plate1
    programFile: "xxx"
- titrate:
    allOf:
    - source: dyeDark
      amount: [0.10ul, 0.15ul, 0.20ul, 0.25ul, 0.30ul, 0.40ul, 0.50ul, 0.75ul, 1.0ul, 1.5ul, 2.0ul, 2.5ul, 3.0ul]
    destination: plate1(A01|H12)
    tip: [5, 6, 7, 8]
    pipettePolicy: Roboliq_Water_Wet_0050
    cleanBetween: flush
