description: |
  Distribute water from trough to plate2(A01|H01)

labware:
  plate1: troughModel_100ml
  plate2: plateModel_96_nunc_transparent

reagents:
  water: plate1(C01|F01)

protocol:
- distribute:
    source: water
    destination: plate2(A01|H01)
    volume: [10ul, 20ul, 30ul, 40ul, 50ul, 60ul, 70ul, 80ul]
