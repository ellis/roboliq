{
	"tags": {
		"allowUnknownTags": true
	},
	"source": {
		"include": ["tutorials/empty.jsdoc"],
		"exclude": ["*"],
		"includePattern": ".+\\.jsdoc$",
		"excludePattern": "(^|\\/|\\\\)_"
	},
	"plugins": ["plugins/markdown"],
	"templates": {
		"cleverLinks": false,
		"monospaceLinks": false,
		"systemName"      : "Roboliq Source",
		"footer"          : "",
		"copyright"       : "Roboliq Copyright Â© 2016-2017 Ellis Whitehead",
		"navType"         : "vertical",
		"theme"           : "cerulean",
		"linenums"        : true,
		"collapseSymbols" : false,
		"inverseNav"      : true,
		"outputSourceFiles": true
	},
	"opts": {
		"template": "node_modules/ink-docstrap/template",
		"destination": "docs",
		"readme": "jsdoc-readme.md"
	}
}
