\begin_layout Standard
\begin_inset Formula 
\begin{eqnarray*}
P(L_{0}) & = & ...\\
P(L_{i}\mid z_{i}) & = & ...\\
P(z_{1};d_{1}) & = & \text{from measurements}\\
P(z_{0}) & = & \text{from regression}\\
P(L_{0}\mid z_{1};d_{1}) & = & P(z_{1}|L_{0};d_{1})\cdot P(L_{0};d_{1})/P(z_{1};d_{1})\\
 & = & P(z_{1}|L_{0};d_{1})\cdot P(L_{0};d_{1})/P(z_{1};d_{1})\\
P(V_{1}\mid z_{1};d_{1}) & = & P(z_{1}|V_{1};d_{1})\cdot P(V_{1};d_{1})/P(z_{1};d_{1})\\
 & = & P(z_{1}|V_{1})\cdot P(V_{1};d_{1})/P(z_{1};d_{1})\\
P(V_{1:2}\mid z_{1:2};d_{1:2}) & = & P(z_{1:2}|V_{1:2};d_{1:2})\cdot P(V_{1:2};d_{1:2})/P(z_{1:2};d_{1:2})\\
 & = & P(z_{1}\mid V_{1};d_{1})\cdot P(z_{2}\mid z_{1},V_{1:2};d_{1:2})\cdot\left(\prod_{i}P(V_{i};d_{i})\right)/\left(P(z_{1};d_{1})\cdot P(z_{2}\mid z_{1};d_{1:2})\right)\\
P(V_{1:n}\mid z_{1:n};d_{1:n}) & = & P(z_{1:n}\mid V_{1:n};d_{1:n})\cdot P(V_{1:n};d_{1:n})/P(z_{1:n};d_{1:n})\\
 & = & P(z_{1:n}\mid V_{1:n})\cdot\left(\prod_{i}P(V_{i};d_{i})\right)/P(z_{1:n};d_{1:n})\\
P(V_{i}\mid z_{1:n};d_{1:n}) & = & P(z_{1:n}\mid V_{i};d_{1:n})\cdot P(V_{i};d_{1:n})/P(z_{1:n};d_{1:n})\\
 & = & P(z_{1:n}\mid V_{i};d_{1:n})\cdot P(V_{i};d_{i})/P(z_{1:n};d_{1:n})
\end{eqnarray*}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{eqnarray*}
P(v_{1}\mid z_{1};d_{1}) & = & P(z_{1}\mid v_{1};d_{1})\cdot P(v_{1};d_{1})/P(z_{1};d_{1})\\
 & = & P(z_{1}\mid v_{1})\cdot P(v_{1};d_{1})/P(z_{1};d_{1})\\
P(v_{1:2}\mid z_{1:2};d_{1:2}) & = & P(z_{1:2}\mid v_{1:2};d_{1:2})\cdot P(v_{1:2};d_{1:2})/P(z_{1:2};d_{1:2})\\
 & = & P(z_{1}\mid v_{1})\cdot P(z_{2}\mid z_{1},v_{1:2})\cdot\left(\prod_{i}P(v_{i};d_{i})\right)/\left(P(z_{1};d_{1})\cdot P(z_{2}\mid z_{1};d_{1:2})\right)\\
P(v_{1:n}\mid z_{1:n};d_{1:n}) & = & P(z_{1:n}\mid v_{1:n};d_{1:n})\cdot P(v_{1:n};d_{1:n})/P(z_{1:n};d_{1:n})\\
 & = & \left(\sum_{z_{0}}P(z_{0})\cdot P(z_{1:n}\mid z_{0},v_{1:n})\right)\cdot\left(\prod_{i}P(v_{i};d_{i})\right)/\left(\prod_{i}P(z_{i}\mid z_{1:i-1},d_{1:i})\right)\\
 & = & \left(\sum_{z_{0}}P(z_{0})\cdot\prod_{i}P(z_{i}\mid z_{0},\sum_{j=1}^{i}v_{j})\right)\cdot\left(\prod_{i}P(v_{i};d_{i})\right)/\left(\prod_{i}P(z_{i}\mid z_{1:i-1},d_{1:i})\right)
\end{eqnarray*}

\end_inset


